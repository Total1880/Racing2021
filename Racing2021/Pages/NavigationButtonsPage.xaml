<Page x:Class="Racing2021.Pages.NavigationButtonsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Racing2021.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NavigationButtonsPage"
      DataContext="{Binding NavigationButtons, Source={StaticResource Locator}}">

    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button
                Content="Home"
                Width="100"
                Command="{Binding OpenHomePageCommand}"/>
            <Button 
                Content="Editor Home"
                Width="100"
                Command="{Binding OpenEditorHomePageCommand}"/>
        </StackPanel>
    </Grid>
</Page>

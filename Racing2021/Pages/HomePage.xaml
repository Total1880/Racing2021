<Page x:Class="Racing2021.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Racing2021.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomePage"
      DataContext="{Binding HomePage, Source={StaticResource Locator}}">

    <Grid>
        <StackPanel>
            <Button 
                Content="Go to racepage" 
                Command="{Binding OpenGameHomeScreenPageCommand}"/>
            <Button 
                Content="Editor" 
                Command="{Binding OpenEditorHomePageCommand}"/>
        </StackPanel>
    </Grid>
</Page>
